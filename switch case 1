let paisDeOrigem 
switch (paisDeOrigem) {
    case 'Brasil':
        console.log("Brasileiro");
        break;        
    case'EUA':
    console.log("norte americano");
        break;
    case'Inglaterra ':
        console.log("ingles ");
        break;
    default:
        console.log("nacionalidade nao encontrada");
        break
        

                
        
}
